let Z = lam f ->
  let g = lam x -> app(f, lam y -> app(app(x, x), y)) in
  app(g, g) in

let mulgen = lam mul -> lam x -> lam y ->
  if y == 0 then 0 else
  if y == 1 then x else
    let z = add(y, -1) in
    add(app(app(mul, x), z), x) in

let mul = app(Z, mulgen) in

let factgen = lam fact -> lam x ->
  if x == 1 then 1 else
    let y = add(x, -1) in
    app(app(mul, x), app(fact, y)) in

let fact = app(Z, factgen) in

app(fact, 6)
