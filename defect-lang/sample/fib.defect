let Z = lam f ->
  let g = lam x -> app(f, lam y -> app(app(x, x), y)) in
  app(g, g) in

let fibgen = lam fib -> lam x ->
  if x == 1 then 1 else
  if x == 2 then 1 else
    let y = add(x, -1) in
    let z = add(x, -2) in
    add(app(fib, y), app(fib, z)) in

let fib = app(Z, fibgen) in

app(fib, 8)
